{
  "name": "TOEFL-Modder",
  "private": true,
  "version": "1.0.0",
  "main": "main.js",
  "build": {
    "appId": "TOEFL-Modder",
    "productName": "TOEFL-Modder",
    "directories": {
      "output": "build"
    },
    "asar": false,
    "win": {
      "icon": "./logo.ico",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "extraResources": [
      {
        "from": "./static/",
        "to": "static"
      },
      {
        "from": "./data/",
        "to": "data"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./logo.ico",
      "installerHeaderIcon": "./logo.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "TOEFL-Modder"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "nodemon --exec electron . --ext .js,.vue,.html,.css",
    "electron:build": "vite build && electron-builder --win --ia32",
    "start": "electron ."
  },
  "dependencies": {
    "axios": "^1.6.2",
    "echarts": "^5.4.3",
    "electron-store": "^8.1.0",
    "vue": "^3.2.45",
    "vue-echarts": "^6.6.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^24.9.1",
    "naive-ui": "^2.35.0",
    "vite": "^4.0.0",
    "vue-router": "^4.1.6"
  }
}
